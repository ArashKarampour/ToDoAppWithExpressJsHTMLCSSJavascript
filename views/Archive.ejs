
    <%- include("./partials/header.ejs",{title:"Archive"}) %>

    <section id="sec-c5d1" class="skrollable skrollable-between u-align-center u-clearfix u-gradient u-section-1" >
      <div class="u-clearfix u-sheet u-sheet-1">
        <h1 class="u-text u-text-body-alt-color u-text-default u-title u-text-1"><b>done tasks</b>
        </h1>
        <ul class="u-align-left u-custom-list u-text u-text-2">
          <% tasks.forEach(task => { %>
            <% if(task.done == true){ %>
          <li>
            <div class="u-list-icon u-list-icon-1">
              <svg class="u-svg-content" viewBox="0 0 512 512" id="svg-971a" style="font-size: 1.5rem; margin: -3em;"><path d="m433.1 67.1-231.8 231.9c-6.2 6.2-16.4 6.2-22.6 0l-99.8-99.8-78.9 78.8 150.5 150.5c10.5 10.5 24.6 16.3 39.4 16.3 14.8 0 29-5.9 39.4-16.3l282.7-282.5z" fill="currentColor"></path></svg>
            </div>
            <span style="font-size: 1.5rem;"><%=task.subject %></span>
            <br>
            <span>
              <a class="my-btn btn-del" href="/api/todo/tasks/delete/<%=task._id %>">Delete</a>
            </span>
          </li>
          <% } %>
          <% }) %>
        </ul>
      </div>
    </section>
    
    <script src="/js/mypanel-archive.js"></script>

    <%- include("./partials/footer.ejs") %>
